<!DOCTYPE html>
  <html>
  <head>
    
    <title>Felixkoders</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-type" content="text/html; charset=utf-8">
    <meta content="" name="description" />
    <meta content="" name="author" />

    <link href="https://fonts.googleapis.com/css?family=Oswald:300,400,500,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,400,500,600,700,800,900" rel="stylesheet">

    <link href="https://fonts.googleapis.com/css?family=Roboto:200,300,400,400i,500,500i,700" rel="stylesheet">

    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
    <!-- --------------------loader-------------------- -->

    <div class="loading-box text-center">
      <div class="loader loader-1 v-middle">
          <div class="loader-outter"></div>
          <div class="loader-inner"></div>
        </div>
    </div>

<!-- ------------------headbar |header | navbar---------------------------- -->
    <div class="head-bar"></div>

    <header id="header-section" class="header-section cd-auto-hide-header clearfix">
      <!-- header-middle - start -->
      <div class="header-middle">
        <div class="container">
          <div class="row">
            <!-- brand-logo-area - start -->
            <div class="col-lg-3 col-sm-3 col-xs-3">
              <div class="brand-logo-area">
                 <%= link_to image_tag('logo.png') , root_path , class: 'brand-logo' , alt: 'Felix Logo' %>
              </div>
            </div>
            <!-- brand-logo-area - end -->
            <div class="col-lg-9 col-sm-9 col-xs-9">
              <!-- contact-info - start -->
              <div class="contact-info pull-right">
                <ul class="">
                   <li><%= link_to 'info@FelixKoders.ae' , 'mailto:info@zaiyna.ae' , class: 'fa fa-envelope'  %></li>
                   <li><%= link_to '8009274  (Toll Free)' , 'tel:8009274' , class: 'fa fa-phone' %></li>
                  </ul>
              </div>
              <!-- contact-info - end -->
            </div>

          </div>
        </div>
      </div>
      <!-- header-middle - end -->
      <!-- header-bottom - start -->
      <div class="header-bottom" id="navbar">
        <div class="container">
          <!-- mainmenu - start -->
          <div class="mainmenu clearfix">
            
          <nav class="navbar">
              <div class="container-fluid">
              <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span> 
                </button>
              </div>
              <div class="collapse navbar-collapse" id="myNavbar">
                <ul class="nav navbar-nav">
                <li><%= link_to 'Home' , root_path , class: 'active' %></li>
                <li><%= link_to 'About Us' , about_path %></li>
                <li><%= link_to 'Our Services' , ourservices_path %></li>
                <li><%= link_to 'Our Offers' , ouroffers_path  %></li>
                <li><%= link_to 'Blog' , blog_path  %></li>
                <li><%= link_to 'Contact Us' , contactus_path  %></li>
                </ul>

                  <div class="top-social pull-right">
                <%= link_to '' , 'https://www.facebook.com/ZaiynaLaundry/' , class: 'fa fa-facebook' , target: '_blank' %>
                <%= link_to '' , 'https://plus.google.com/+ZaiynaAe800WASH' , class: 'fa fa-google-plus' , target: '_blank' %>
                <%= link_to '' , 'https://twitter.com/Laundry_JLT' , class: 'fa fa-twitter' , target: '_blank' %>
                  </div>
                </div>
              </div>
          </nav>
            
          </div>
          <!-- mainmenu - end -->
        </div>
      </div>
      <!-- header-bottom - end -->
    </header>


    <%= yield %>


